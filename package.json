{
  "name": "@nahanil/namesilo",
  "version": "0.0.1",
  "description": "Interact with the NameSilo API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "jest --watch",
    "build": "npm test && npm run readme",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "doc": "node ./.namesilo/doc.js && jsdoc index.js lib --ignore ./lib/utils.js",
    "readme": "node ./.namesilo/doc.js && cat .namesilo/README.md > README.md && doxdox -l markdown ./lib -i lib/utils.js | sed -e 's/# lib.xx_autogen.js/ Autogenerated Methods/g' | grep -v 'namesilo.js' >> README.md"
  },
  "author": "Jarrod Linahan",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "doxdox": "^3.0.0",
    "jest": "^24.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/texh/node-namesilo.git"
  },
  "bugs": {
    "url": "https://github.com/texh/node-namesilo/issues"
  },
  "homepage": "https://github.com/texh/node-namesilo"
}
